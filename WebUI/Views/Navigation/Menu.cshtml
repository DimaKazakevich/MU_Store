@model IEnumerable<string>

<li class="menuHeader">
@Html.ActionLink("Home", "List", "Products", null,
	new { @class = "navItem" })
</li>

@foreach (var link in Model)
{
	<li class="menuHeader">
		@Html.RouteLink(link, new
		{

			controller = "Products",
			action = "List",
			category = link,
			page = 1
		}, new
		{
			@class = "navItem"
		})
	</li>
}